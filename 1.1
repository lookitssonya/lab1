#include <iostream>
#include <queue>
using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    queue<int> myQueue;
    int size, value, countEven = 0;
    cout << "Введите размер очереди: ";
    cin >> size;
    cout << "Введите " << size << " элементов: ";
    for (int i = 0; i < size; i++) {
        cin >> value;
        myQueue.push(value);
    }
    while (!myQueue.empty()) {
        if (myQueue.front() % 2 == 0) {
            countEven++;
        }
        myQueue.pop();
    }
    cout << "Количество четных чисел в очереди: " << countEven << endl;
    return 0;
}
